    "spotify",
    "soundcloud",
    "mixcloud"
  ],
  "version": "1.1.1",
  "private": false,
  "dependencies": {
    "@types/node": "^18.16.0",
    "@types/react": "^18.0.38",
    "datocms-plugin-sdk": "^0.7.7",
    "datocms-react-ui": "^0.7.7",
    "lodash": "^4.17.21",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
